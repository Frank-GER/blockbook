{{define "main" -}}
#!/usr/bin/make -f

DH_VERBOSE = 1
# nostrip is needed for pkg-compiled binary to work
DEB_BUILD_OPTIONS="debug nostrip noopt"

%:
	dh $@ --with=systemd

override_dh_systemd_start:
	dh_systemd_start --no-start

override_dh_installinit:

override_dh_strip:
	dh_strip --no-automatic-dbgsym
{{end}}
